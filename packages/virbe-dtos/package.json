{
    "name": "@virbe/dtos",
    "version": "0.0.1",
    "private": true,
    "type": "module",
    "scripts": {
        "build": "tsup src/*/index.ts --format cjs,esm",
        "dev": "npm run build -- --watch"
    },
    "exports": {
        "./*": {
            "types": "./src/*/index.ts",
            "module": "./dist/*/index.mjs",
            "import": "./dist/*/index.js",
            "require": "./dist/*/index.cjs"
        }
    },
    "devDependencies": {
        "@types/node": "^20.12.7",
        "@typescript-eslint/eslint-plugin": "^7.18.0",
        "@typescript-eslint/parser": "^7.18.0",
        "eslint": "^8.57.0",
        "prettier": "^3.3.3",
        "typescript": "^5.6.2",
        "tsup": "^8.0.2",
        "big.js": "^6.2.1",
        "@types/big.js": "^6.2.2"
    },
    "dependencies": {
        "zod": "^4.1.12"
    }
}
